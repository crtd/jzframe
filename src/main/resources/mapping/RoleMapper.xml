<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.musikouyi.jzframe.dao.mapper.RoleMapper">
    <select id="findByDeptId" resultType="role" parameterType="Integer">
        select * from sys_role where dept_id = #{deptId}
    </select>
</mapper>